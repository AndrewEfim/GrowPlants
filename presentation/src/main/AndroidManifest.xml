<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.neversaydie.andreii.growplants">

    <application
        android:name=".presentation.app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_grow_plant_regt"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_grow_plant_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.screen.GameMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="[ADMOB_APP_ID]"/>
        <meta-data android:name="google_analytics_adid_collection_enabled" android:value="false" />

    </application>

</manifest>